    "name" : "Kalin Maldzhanski"
  }, {
    "@id" : "ofd:NonFoodItemType",
    "@type" : "owl:Class",
    "label" : "Non-Food Item"
  }, {
    "@id" : "ofd:OFD",
    "@type" : "dcterms:Agent",
    "homepage" : "http://www.openfooddata.org/",
    "member" : "ofd:Kalin",
    "name" : "Open Food Data"
  }, {
    "@id" : "ofd:actorType",
