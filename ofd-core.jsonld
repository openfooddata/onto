  }, {
    "@id" : "ofd:FoodActorType",
    "@type" : "owl:Class",
    "comment" : "base class for hierarchical classification of Food Actors",
    "label" : "Food Actor"
  }, {
    "@id" : "ofd:FoodProduct",
    "@type" : "owl:Class",
    "comment" : "base class for Food Products. Instances are not actual Offerings. Instances might also be of type schema:Product",
    "label" : "Food Product"
  }, {
    "@id" : "ofd:FoodService",
    "@type" : "owl:Class",
    "comment" : "base class for Food Services. Instances are not actual Offerings.",
    "label" : "Food Service"
  }, {
    "@id" : "ofd:FoodType",
    "@type" : "owl:Class",
    "comment" : "base class for hierarchical classification of Food Types",
    "label" : "Food Type"
