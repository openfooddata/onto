    "domain" : "ofd:FoodProduct",
    "label" : "broaderFoodType",
    "range" : "ofd:FoodType",
    "subPropertyOf" : "skos:broadMatch",
    "prefLabel" : {
      "@language" : "en",
      "@value" : "food category"
    }
  }, {
    "@id" : "ofd:canHandleFoodType",
    "@type" : "owl:ObjectProperty",
