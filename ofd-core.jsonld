    "domain" : "ofd:FoodServiceType",
    "label" : "quantitativeRestriction",
    "range" : "qudt:Value",
    "subPropertyOf" : "skos:semanticRelation"
  }, {
    "@id" : "ofd:relatedFoodProduct",
    "@type" : "owl:ObjectProperty",
    "comment" : "relates concept to a FoodService",
    "domain" : "skos:Concept",
    "label" : "relatedFoodProduct",
    "range" : "ofd:FoodProduct",
    "subPropertyOf" : "skos:semanticRelation"
  }, {
    "@id" : "ofd:relatedFoodType",
    "@type" : "owl:ObjectProperty",
    "comment" : "relates concept to a FoodService",
    "domain" : "skos:Concept",
    "label" : "relatedFoodType",
    "range" : "ofd:FoodType",
    "subPropertyOf" : "skos:semanticRelation"
  }, {
    "@id" : "ofd:relatedService",
    "@type" : "owl:ObjectProperty",
    "comment" : "relates concept to a FoodService",
    "domain" : "skos:Concept",
    "label" : "relatedService",
    "range" : "ofd:FoodServiceType",
    "subPropertyOf" : "skos:semanticRelation"
  }, {
    "@id" : "ofd:storageDuration",
    "@type" : "owl:ObjectProperty",
    "comment" : "the allowed storage before consumption min and max or more values may be used",
    "domain" : "ofd:FoodProduct",
    "label" : "storageDuration",
    "range" : "qudt:Value"
  }, {
    "@id" : "ofd:storageTemp",
    "@type" : "owl:ObjectProperty",
    "comment" : "Temp range depending on the cold/heat tolerance of the food min and max or more values may be used",
    "domain" : "ofd:FoodProduct",
    "label" : "storageTemp",
    "range" : "qudt:Value"
  }, {
    "@id" : "ofd:usedInMaking",
    "@type" : "owl:ObjectProperty",
    "domain" : "ofd:FoodProduct",
    "label" : "usedInMaking",
    "range" : "ofd:FoodProduct",
    "subPropertyOf" : "skos:semanticRelation",
    "inverseOf" : "ofd:madeOf"
  } ],
