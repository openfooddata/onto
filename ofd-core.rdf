<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:schema="https://schema.org/"
    xmlns:ofd="https://w3id.org/openfooddata/onto/core#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:adms="http://www.w3.org/ns/adms#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:qudt="http://qudt.org/2.0/schema/qudt#"
    xmlns:vf="https://w3id.org/valueflows/"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Class rdf:about="https://w3id.org/openfooddata/onto/core#FoodType">
    <rdfs:comment>base class for hierarchical classification of Food Types</rdfs:comment>
    <rdfs:label>Food Type</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/openfooddata/onto/core#FoodProduct">
    <rdfs:comment>base class for Food Products. Instances are not actual Offerings. Instances might also be of type schema:Product</rdfs:comment>
    <rdfs:label>Food Product</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/openfooddata/onto/core#NonFoodItem">
    <rdfs:label>Non-Food Item</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/openfooddata/onto/core#FoodActorType">
    <rdfs:comment>base class for hierarchical or other classification of Food Actors</rdfs:comment>
    <rdfs:label>Food Actor Type</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/openfooddata/onto/core#FoodService">
    <rdfs:comment>base class for Food Services. Instances are not actual Offerings.</rdfs:comment>
    <rdfs:label>Food Service</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#offersService">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodServiceType"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label>provides Service</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#narrowerFoodProduct">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodType"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#broaderFoodType"/>
    </owl:inverseOf>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#narrowMatch"/>
    <skos:prefLabel xml:lang="en">related food product</skos:prefLabel>
    <rdfs:label>narrowerFoodProduct</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#claimedBy">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment>Identifies the Agent who claimed ownership on this Food Actor and its data</rdfs:comment>
    <rdfs:label>claimedBy</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#actorType">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodActorType"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label>actor Type</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#canHandleFoodType">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodType"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodServiceType"/>
    <rdfs:comment>indicates that specific food taype is can be handled by this service</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
    <rdfs:label>canHandleFoodType</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#storageDuration">
    <rdfs:range rdf:resource="http://qudt.org/2.0/schema/qudt#Value"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:comment>the allowed storage before consumption min and max or more values may be used</rdfs:comment>
    <rdfs:label>storageDuration</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#preparationTime">
    <rdfs:range rdf:resource="http://qudt.org/2.0/schema/qudt#QuantityValue"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodActor"/>
    <rdfs:comment>the min number of day/hours for the Actor to be available after commitment</rdfs:comment>
    <rdfs:label>harvestPeriod</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#canNotHandleFoodType">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodType"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodServiceType"/>
    <rdfs:comment>indicates that specific food taype is cannot be handled by this service</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
    <rdfs:label>canNotHandleFoodType</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#madeOf">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:label>madeOf</rdfs:label>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#usedInMaking"/>
    </owl:inverseOf>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#relatedService">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodServiceType"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:comment>relates concept to a FoodService</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
    <rdfs:label>relatedService</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#relatedFoodProduct">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:comment>relates concept to a FoodService</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
    <rdfs:label>relatedFoodProduct</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#producedBy">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:label>producedBy</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#usedInMaking">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:label>usedInMaking</rdfs:label>
    <owl:inverseOf rdf:resource="https://w3id.org/openfooddata/onto/core#madeOf"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#storageTemp">
    <rdfs:range rdf:resource="http://qudt.org/2.0/schema/qudt#Value"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:comment>Temp range depending on the cold/heat tolerance of the food min and max or more values may be used</rdfs:comment>
    <rdfs:label>storageTemp</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#offersProduct">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label>offers Product</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#relatedFoodType">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodType"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:comment>relates concept to a FoodService</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
    <rdfs:label>relatedFoodType</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#producesFoodType">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodType"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label>offers FoodType</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#harvestTime">
    <rdfs:range rdf:resource="http://qudt.org/2.0/schema/qudt#QuantityValue"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:comment>the min number of day/hours for the product to be available after purchase</rdfs:comment>
    <rdfs:label>harvestPeriod</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#broaderFoodType">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#FoodType"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodProduct"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#broadMatch"/>
    <skos:prefLabel xml:lang="en">food category</skos:prefLabel>
    <rdfs:label>broaderFoodType</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#quantitativeRestriction">
    <rdfs:range rdf:resource="http://qudt.org/2.0/schema/qudt#Value"/>
    <rdfs:domain rdf:resource="https://w3id.org/openfooddata/onto/core#FoodServiceType"/>
    <rdfs:comment>custom restrictions that the Service can handle min and max or more values may be used</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
    <rdfs:label>quantitativeRestriction</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#offersNonFoodItem">
    <rdfs:range rdf:resource="https://w3id.org/openfooddata/onto/core#NonFoodItem"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label>offersNonFoodItem</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/openfooddata/onto/core#operationalCoverage">
    <rdfs:range rdf:resource="https://schema.org/GeoShape"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment>activity coverage of a specific actor, which may depend on its agreements with other actors</rdfs:comment>
    <rdfs:label>operationalCoverage</rdfs:label>
  </owl:ObjectProperty>
  <owl:DataProperty rdf:about="https://w3id.org/openfooddata/onto/core#isLookingForPartners">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label>isLookingForPartners</rdfs:label>
  </owl:DataProperty>
  <adms:SemanticAsset rdf:about="https://w3id.org/openfooddata/core">
    <dcterms:partOf rdf:resource="https://w3id.org/openfooddata/"/>
    <vann:preferredNamespacePrefix>ofd</vann:preferredNamespacePrefix>
    <dc:license>https://creativecommons.org/publicdomain/</dc:license>
    <foaf:homepage rdf:resource="https://w3id.org/openfooddata/core"/>
    <dc:creator>
      <foaf:Person rdf:about="https://w3id.org/openfooddata/onto/core#Kalin">
        <foaf:homepage rdf:resource="http://djodjo.org"/>
        <owl:SameAs rdf:resource="http://djodjo.org#az"/>
        <foaf:name>Kalin Maldzhanski</foaf:name>
      </foaf:Person>
    </dc:creator>
    <dcterms:type rdf:resource="http://purl.org/adms/assettype/Ontology"/>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2017-04-18</dcterms:modified>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2017-02-17</dcterms:created>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dcterms:title>Open Food Data Ontology</dcterms:title>
    <vann:preferredNamespaceUri>https://w3id.org/openfooddata/core</vann:preferredNamespaceUri>
    <dcterms:description>This ontology models the Open Food Data domain. It allows to describe food products, food producers/farmers, food shops/hubs and other actors and collaboration agreements between those</dcterms:description>
    <dcterms:status rdf:resource="http://purl.org/adms/status/UnderDevelopment"/>
    <dcterms:publisher>
      <dcterms:Agent rdf:about="https://w3id.org/openfooddata/onto/core#OFD">
        <foaf:homepage rdf:resource="http://www.openfooddata.org/"/>
        <foaf:name>Open Food Data</foaf:name>
        <foaf:member rdf:resource="https://w3id.org/openfooddata/onto/core#Kalin"/>
      </dcterms:Agent>
    </dcterms:publisher>
  </adms:SemanticAsset>
</rdf:RDF>
